    private void jB_searchActionPerformed(java.awt.event.ActionEvent evt) {                                          
       
        try{
            String id = jt_id.getText();
        Class.forName("com.mysql.cj.jdbc.Driver");
            Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Employee_managment","root","Pri#yal%1*8nam_04$");
            
            PreparedStatement ps=cn.prepareStatement("select * from employee where id=?");
            ps.setString(1, id);
            ResultSet rs=ps.executeQuery();
            if(rs.next())
            {
            String name=rs.getString(2);
            String email=rs.getString(3);
            String mobile=rs.getString(4);
            String salary=rs.getString(5);
            
            jt_name.setText(name);
            jt_email.setText(email);
            jt_mobile.setText(mobile);
            jt_salary.setText(salary);
            
            }
            cn.close();
        }
        catch(Exception e){
            System.out.println(e.getMessage());
            
            
        }
    

        
    }                                         

