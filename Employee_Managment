    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        try{
        String id=jt_id.getText();
        String name=jt_name.getText();
        String email=jt_email.getText();
        String mobile=jt_mobile.getText();
        String salary=jt_salary.getText();
     Class.forName("com.mysql.cj.jdbc.Driver");
            Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Employee_managment","root","Pri#yal%1*8nam_04$");
            
            PreparedStatement ps=cn.prepareStatement("update employee set name=?,email=?,mobile=?,salary=? where id=?");
            ps.setString(1, name);
            ps.setString(2, email);
            ps.setString(3, mobile);
            ps.setString(4, salary);
            ps.setString(5, id);
            
           boolean b=ps.execute();
            if(b==false)
            {
                JOptionPane.showMessageDialog(rootPane, "Employee has been Updated");
            }            
            
            
            
            cn.close();
        }
        catch(Exception e){
            System.out.println(e.getMessage());
            
            
        }        
        
        
        
        
    }                                        

