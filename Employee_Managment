    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
         try{
        String id=d_id.getText();

     Class.forName("com.mysql.cj.jdbc.Driver");
            Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Employee_managment","root","Pri#yal%1*8nam_04$");
            
            PreparedStatement ps=cn.prepareStatement("delete from employee where id=?");
            ps.setString(1, id);
           
           boolean b=ps.execute();
            if(b==false)
            {
                JOptionPane.showMessageDialog(rootPane, "data is deleted");
            }            
            
            
            
            cn.close();
        }
        catch(Exception e){
            System.out.println(e.getMessage());
            
            
        }        
        
    }                                        

    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       try{
            Class.forName("com.mysql.cj.jdbc.Driver");
            Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Employee_managment","root","Pri#yal%1*8nam_04$");
            PreparedStatement ps=cn.prepareStatement("select * from employee");
            ResultSet rs=ps.executeQuery();
           
            DefaultTableModel model=(DefaultTableModel) jTable1.getModel();
            model.setRowCount(0);
           
            while(rs.next()){
            model.addRow(new String[]{rs.getString(1),rs.getString(2),rs.getString(3),rs.getString(4),rs.getString(5)});
           
        }
        }
        catch(Exception e){
                System.out.println(e.getMessage());
                }
       
    }                                        

